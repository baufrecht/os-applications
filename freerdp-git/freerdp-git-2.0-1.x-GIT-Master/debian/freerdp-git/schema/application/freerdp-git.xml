            <label lang="de" value="Serveradresse"/>
            <label lang="en" value="server address"/>
        </entry>
        <entry name="Port">
            <label lang="de" value="alternativer Port"/>
            <label lang="en" value="alternative port"/>
        </entry>
        <group name="Window">
            <label lang="de" value="Fenster"/>
            <label lang="en" value="Window"/>
